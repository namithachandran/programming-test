<%= form_with scope: :student,url: students_path, local: true do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
 
  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </p>

      <p>
    <%= form.label :subject %><br>
    <%= form.text_field :subject %>
  </p>

  <p>
    <%= form.label :mark %><br>
    <%= form.text_field :mark %>
  </p>

  <p>
    <%= form.submit %>
  </p>
<% end %>